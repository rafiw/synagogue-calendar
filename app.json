{
  "expo": {
    "scheme": "synagogue-calendar",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./docs/images/app-logo.png"
    },
    "experiments": {
      "baseUrl": "/synagogue-calendar"
    },
    "assetBundlePatterns": ["**/*"],
    "name": "synagogue-calendar",
    "slug": "synagogue-calendar",
    "userInterfaceStyle": "light",
    "plugins": ["expo-router", "@react-native-community/datetimepicker"],
    "android": {
      "package": "com.jewish.synagoguecalendar",
      "intentFilters": [
        {
          "action": "android.intent.action.MAIN",
          "category": ["android.intent.category.LEANBACK_LAUNCHER", "android.intent.category.LAUNCHER"]
        }
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c9f056f-f5ff-40e5-93ea-e11c9987769e"
      }
    },
    "owner": "rafi1082"
  }
}
